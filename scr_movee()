var hsp = argument[0];
var vsp = argument [1];


if place_meeting(x+hsp, y, oWall1)
{
	while(!place_meeting(x + sign(hsp),y,oWall1))	
	{
	x	+= hsp;
	}
	hsp = 0;

}



x+=hsp;

if place_meeting(x, y + sign(vsp), oWall1)
{
	while(!place_meeting(x,y+ sign(vsp),oWall1))	
	{
	y+= vsp;
	}
	vsp = 0;

}

y += vsp;
